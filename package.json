  "bugs": {
    "url": "https://github.com/seejins/VolunteerHours-Server/issues"
  },
  "homepage": "https://github.com/seejins/VolunteerHours-Server#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "helmet": "^3.21.3",
    "knex": "^0.20.10",
    "morgan": "^1.9.1",
    "pg": "^7.18.2",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
